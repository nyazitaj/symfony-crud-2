{% extends 'base.html.twig' %}

{% block title %}Hello FilmsController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="films">
    {% for film in films %}
        <div class="film-container">
            <div class="row-film-image">
                <span>
                    <img src="data:image/jpeg;base64," alt="Batman" />
                </span>
            </div>

            <div class="row-film-info">
                <span>
                    {{ film.title }}
                </span>
                <span>
                    {{ film.realisateur }}
                </span>
                <span>
                    {{ film.genre }}
                </span>
            </div>
        </div>
    {% endfor %}
</div>
{% endblock %}
